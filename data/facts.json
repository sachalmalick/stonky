{
    "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
    "type": "AdaptiveCard",
    "version": "1.3",
    "body": [
        {
            "speak": "Stock News",
            "type": "ColumnSet",
            "columns": [
                {
                    "type": "Column",
                    "width": 2,
                    "items": [
                        {
                            "type": "Image",
                            "url": "{logo}",
                            "altText": "{imageAlt}",
                            "size": "Large",
                            "spacing": "Medium",
                            "horizontalAlignment": "Center"
                        },
                        {
                            "type": "TextBlock",
                            "text": "{ticker}",
                            "weight": "Bolder",
                            "size": "ExtraLarge",
                            "spacing": "None",
                            "wrap": true,
                            "horizontalAlignment": "Center"
                        },
                        {
                            "type": "TextBlock",
                            "$when": "2020-03-27T09:41:00.000Z",
                            "text": "{CEO}",
                            "isSubtle": true,
                            "spacing": "None",
                            "wrap": true,
                            "horizontalAlignment": "Center"
                        },
                        {
                        "type": "FactSet",
                        "facts": [
                            
                        ]
                    }
                    ]
                }
            ]
        }
        
    ]
}

                            
